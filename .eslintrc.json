{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/typescript"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/consistent-type-imports": 1,
    "react/no-unknown-property": ["error", { "ignore": ["jsx"] }],
  }
}
